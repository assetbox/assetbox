{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "test:type": {},
    "test": {
      "inputs": ["packages/tools/**/*.ts"]
    },
    "@assetbox/trpc#build": {
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["@assetbox/trpc#build"],
      "outputs": ["dist/**", "bin/**"]
    }
  }
}
